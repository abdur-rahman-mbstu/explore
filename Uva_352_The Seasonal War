#include<bits/stdc++.h>
using namespace std;
int cell[30][30],vis[30][30],n;
int fx[]= {0,0,1,-1,1,-1,1,-1},fy[]= {1,-1,0,0,1,1,-1,-1};
void dfs(int p, int q)
{
    int i,j,x,y;
    vis[p][q]=1;
    for(i=0; i<8; i++)
    {
        x= p+fx[i];
        y= q+fy[i];
        //cout<<x<<" "<<y<<endl;
        if(x>=0&&x<n&&y>=0&&y<n&&!vis[x][y]&&cell[x][y]==1) dfs(x,y);
    }
}
int main()
{
    int m,i,j,cnt,t=1;
    string c;
    while(cin>>n)
    {
        cnt=0;
        memset(vis,0,sizeof(vis));
        for(i=0; i<n; i++)
        {
            cin>>c;
            for(j=0; j<n; j++)
            {
                cell[i][j] = c[j]-'0';
            }
        }
        for(i=0; i<n; i++)
        {
            for(j=0; j<n; j++)
            {
                if(cell[i][j]==1&& !vis[i][j])
                {
                    cnt++;
                    //cout<<i<<" "<<j<<endl;
                    dfs(i,j);
                }
            }
        }
        cout<<"Image number "<<t++<<" contains "<<cnt<<" war eagles."<<endl;
    }
    return 0;
}
